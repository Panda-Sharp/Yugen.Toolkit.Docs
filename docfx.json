{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "UwpCommunity.Uwp.Controls/**.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "docs/**"
          ],
          "src": "../Toolkit"
        }
      ],
      "properties": {
        "TargetFrameworks": "netstandard2.0"
      },
      "dest": "metadata/uwp.controls"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "index.md",
          "metadata/toc.yml",
          "metadata/index.md",
          "articles/**/toc.yml",
          "articles/**.md",
          "metadata/uwp.controls/*.yml",
          "metadata/uwp.controls/index.md"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "overwrite/**.md"
        ]
      }
    ],
    "dest": "docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/darkfx"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false
  }
}